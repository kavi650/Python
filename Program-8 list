student = []

student.append("kavin")
student.append("kiran")
student.append("navanee")
student.append("kavi")

for i in student:
    print(i)

student.insert(1, "giri")
print(student)

student.remove("kavi")
print(student)

student.sort()
print(student)

for i in student:
    print(i)

a = input("Enter the name: ")

for i in student:
    if i == a:
        print("The name is present in the list")
        break
else:
    print("The name is not present in the list")
